{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "next-commerce#build": {
      "dependsOn": [
        "^build",
        "$COMMERCE_PROVIDER",
        "$BIGCOMMERCE_STOREFRONT_API_URL",
        "$NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
        "$NEXT_PUBLIC_SWELL_STORE_ID"
      ],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "prettier-fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "types": {
      "outputs": ["dist/**/*.d.ts"]
    }
  }
}
